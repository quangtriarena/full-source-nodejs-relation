{
  "name": "full-crud-api",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "start": "cross-env NODE_ENV=production nodemon --exec babel-node --inspect server.js --port 5006",
    "test": "echo \"Error: no test specified\" && exit 1",
    "ngrok": "ngrok http --subdomain=arena-app-tt --region=ap 8080",
    "dev": "concurrently \"yarn ngrok\" \"yarn start\"",
    "bulk_products": "node ./src/mocks/productGenerate.js",
    "bulk_categories": "node ./src/mocks/categoryGenerate.js",
    "bulk_stores": "node ./src/mocks/storeGenerate.js",
    "bulk_product_store": "node ./src/mocks/productStoreGenerate.js",
    "bulk_customer": "node ./src/mocks/customerGenerate.js",
    "bulk_store_setting": "node ./src/mocks/storeSettingGenerate.js",
    "bulk_product_shopify": "node ./src/mocks/productShopifyGenerate.js",
    "bulk_wishlist": "node ./src/mocks/wishlistGenerate.js",
    "bulk_wishlist_product": "node ./src/mocks/wishlistProductGenerate.js",
    "bulk_wishlist_customer": "node ./src/mocks/wishlistCustomerGenerate.js",
    "prettier:code": "prettier \"**/*.{js,jsx}\"",
    "format:code": "npm run prettier:code -- --write",
    "server": "nodemon --exec babel-node --inspect server.js --port 5006",
    "build": "babel src server.js index.js -d build --copy-files",
    "pm2":"pm2 start ./build/server.js --exp-backoff-restart-delay=100"

  },
